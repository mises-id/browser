    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "<rootDir>/app/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/app/util/*.{js,jsx}",
